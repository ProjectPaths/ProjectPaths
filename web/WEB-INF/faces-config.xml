<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>../view/homepage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bean.Next_job_seeker}</from-action>
            <from-outcome>job_seeker</from-outcome>
            <to-view-id>../view/job_seeker.xhtml.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/job_seeker.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bean.job_seeker_to_job_seeker_apply}</from-action>
            <from-outcome>job_seeker_apply</from-outcome>
            <to-view-id>../view/job_seeker_apply.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{test.signup}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>../view/success.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>

        <from-view-id>../view/company_signin_page.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{signin.signin}</from-action>
            <from-outcome>company_login_done</from-outcome>
            <to-view-id>../view/company_login_done.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{signin.signin}</from-action>
            <from-outcome>company_signin_page</from-outcome>
            <to-view-id>../view/company_signin_page.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/company_login_done.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{signin.goback}</from-action>
            <from-outcome>company_signin_page</from-outcome>
            <to-view-id>../view/company_signin_page.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{updatebean.update}</from-action>
            <from-outcome>company_info_update</from-outcome>
            <to-view-id>../view/company_info_update.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{updatebean.job_next}</from-action>
            <from-outcome>job_post</from-outcome>
            <to-view-id>../view/job_post.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/client_signin_signup.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{client.Signup}</from-action>
            <from-outcome>client_signin_signup</from-outcome>
            <to-view-id>../view/client_signin_signup.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{client.signin}</from-action>
            <from-outcome>client_signin_signup</from-outcome>
            <to-view-id>../view/client_signin_signup.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{client.signin}</from-action>
            <from-outcome>client_login_done</from-outcome>
            <to-view-id>../view/client_login_done.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/client_login_done.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{client.logout}</from-action>
            <from-outcome>client_signin_signup</from-outcome>
            <to-view-id>../view/client_signin_signup.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-action>#{updatebean.product}</from-action>
            <from-outcome>company_login_done</from-outcome>
            <to-view-id>../view/company_login_done.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

    <navigation-rule>
        <from-view-id>../view/job_post.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{updatebean.job_post}</from-action>
            <from-outcome>company_login_done</from-outcome>
            <to-view-id>../view/company_login_done.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/search_home.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{search.search_initialize}</from-action>
            <from-outcome>search_result_1</from-outcome>
            <to-view-id>../view/search_result_1.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
      <navigation-rule>
          <from-view-id>../view/search_result_1.xhtml</from-view-id>
          <navigation-case>
               <from-action>#{search.order}</from-action>
              <from-outcome>order2ndpage</from-outcome>
              <to-view-id>../view/order2ndpage.xhtml</to-view-id>
          </navigation-case>
      </navigation-rule>
    <navigation-rule>
        <from-view-id>../view/homepage.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bean.Next_job_seeker}</from-action>
            <from-outcome>job_seeker</from-outcome>
            <to-view-id>../view/job_seeker.job_seeker_apply.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>